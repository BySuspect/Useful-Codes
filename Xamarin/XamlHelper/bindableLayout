                        <StackLayout>
                            <Label FontSize="Large" TextColor="White">
                                <Label.FormattedText>
                                    <FormattedString>
                                        <Span Text="{Binding bolum}" />
                                    </FormattedString>
                                </Label.FormattedText>
                            </Label>
                            <Label
