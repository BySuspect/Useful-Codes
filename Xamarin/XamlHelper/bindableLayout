     <ScrollView>
        <StackLayout x:Name="test" BindableLayout.ItemsSource="{Binding MyItems}">
            <BindableLayout.ItemTemplate>
                <DataTemplate>
                    <Frame
                        BackgroundColor="red"
                        BorderColor="White"
